 <div class="container">
  <div class="row">
   <div class="col-md-3">
    <%= render 'users/list', user:@user%>
    <%= render 'books/list', book:@book %>
   </div>

   <div class="col-md-8 offset-md-1">
    <h2>Users</h2>
    <table class="table">
     <thead>
      <tr>
       <th>image</th>
       <th>name</th>
       <th colspan="3"></th>
      </tr>
     </thead>
      <tbody>
       <% @users.each do |users| %>
       <tr>
         <th><%= attachment_image_tag users, :profile_image, :fill, 60, 60, format: 'jpeg', fallback: "no_image.jpg" %></th>
         <th>
          <%= users.name %>
         </th>
         <th><%= link_to "show", user_path(users.id) %></th>
       </tr>
       <% end %>
      </tbody>
    </table>
    </div>
   </div>
</div>